{
  "name": "notebook-scanner",
  "version": "0.1.0",
  "description": "Scan notebook pages and convert them to organized notes using LLM vision",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src/",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ocr",
    "notebook",
    "handwriting"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "obsidian": "latest",
    "typescript": "^5.3.3"
  }
}
